# === OpenAI Config ===
OPENAI_ORGANIZATION=your-openai-org-id
OPENAI_API_KEY=your-openai-api-key

# === GCP Config ===
GCP_PROJECT=nidsml
GCP_PROJECT_ID=nidsml
GCP_LOCATION=us-central1
GCP_TABLE_ID=vpc_flow_logs

# === Databricks Config ===
DATABRICKS_CONFIG_PROFILE=your-databricks-profile
DATABRICKS_HOST=https://your-workspace.cloud.databricks.com/
DATABRICKS_TOKEN=your-databricks-token
DATABRICKS_FOREIGN_CATALOG_NAME=bq_catalog
DATABRICKS_WAREHOUSE_ID=4d8138003e5cdaf6

# === Pub/Sub Config ===
GCP_PUBSUB_TOPIC=vpc-flow-logs-stream
GCP_PUBSUB_SUBSCRIPTION=databricks-vpc-logs-sub
GCP_PUBSUB_ENABLE_STREAMING=true
DATABRICKS_ENV_SCOPE=nids_env

# === BigQuery Service Account ===
# Použi JEDNU z možností:
# 1) Cesta k súboru
GCP_BQ_SA_KEY_FILE=~/databricks-bq-reader.json
# 2) Alebo inline JSON (vložený ako string)
# GCP_BQ_SA_KEY_JSON={"type":"service_account","project_id":"nidsml", ... }

# === Optional ===
GCP_SA_NAME=databricks-bq-reader
GCP_SA_DISPLAY_NAME="Databricks BigQuery Reader"

GCP_BQ_CONNECTION_NAME=bigquery_connection
DATABRICKS_CONNECTION_NAME=bigquery_connection
DATABRICKS_CONNECTION_READ_ONLY=false

# Ak máš viac účtov:
# GCP_ACCOUNT_MAIN=slavosmn95@gmail.com

# Extra (ak chceš debugovať BigQuery datasets)
SHOW_BQ_TABLE_PERMISSIONS=false
GCP_DATASETS=vpc_logs

# (Voliteľné – ak potrebuješ email pre granty)
DATABRICKS_EMAIL=your-email@domain.com

DASHBOARD_LOOKBACK_HOURS=720

# === Agent Studio Config ===
AGENT_STUDIO_PATH=/Workspace/Repos/your-email@domain.com/agent-studio/
